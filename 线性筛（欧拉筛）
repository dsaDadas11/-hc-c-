https://www.luogu.com.cn/problem/solution/P3383


#include<iostream>
#include<string>
#include<algorithm>
#include<cstring>
#define endl '\n'
#define ll long long
//#define int ll
using namespace std;
const int N = 1e8 + 7;
bool a[N];
int p[N];
signed main()
{
	ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
	int n, q;
	cin >> n >> q;
	int cnt = 0;
	memset(a, 1, sizeof(a));
	a[1] = 0;
	for (int i = 2;i <= n;i++)
	{
		if (a[i]) p[++cnt] = i;
		for (int j = 1;j <= cnt && (ll)p[j] * i <= n;j++)
		{
			a[p[j] * i] = 0;
			if (!i % p[j]) break;
		}
	}
	while (q--)
	{
		int k;
		cin >> k;
		cout << p[k] << endl;
	}
	return 0;
}
