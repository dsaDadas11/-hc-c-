#include<bits/stdc++.h>
#define endl '\n'
#define ll long long
#define int ll
using namespace std;
const int N=1e6+7;
int n;
int a[N],vis[N];
void dfs(int x)
{
	if(x==n+1)
	{
		for(int i=1;i<=n;i++) cout<<a[i]<<' ';
		cout<<endl;
		return;
	}
	for(int i=1;i<=n;i++)
	{
		if(!vis[i])//表示i号牌还在手里
		{
			a[x]=i;//把i号牌放入x号盒里
			vis[i]=1;//i号牌拿走了
			dfs(x+1);//递归下一个盒子
			vis[i]=0;//回收i号牌
		}
	}
}
signed main()
{
	ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
	while(cin>>n)
	{
		memset(vis,0,sizeof vis);
		dfs(1);
	}
	return 0;
}
