#include<iostream>
#include<algorithm>
#define endl '\n'
#define ll long long
//#define int ll
using namespace std;
const int N=1e6+7;
int a[N];
signed main()
{
	ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
	int t;
	cin>>t;
	while(t--)
	{
		int n,s;
		cin>>n>>s;
		for(int i=1;i<=n;i++) cin>>a[i];
		int res=n+1,l=1,r=1,sum=0;
		while(1)
		{
			while(r<=n&&sum<s)
			{
				sum+=a[r++];
			}
			if(sum<s) break;
			res=min(res,r-l);
			sum-=a[l++];
		}
		if(res>n) res=0;
		cout<<res<<endl;
	}
	return 0;
}
