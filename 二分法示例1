#include<iostream>
#include<algorithm>
#define endl '\n'
#define ll long long
//#define int ll
using namespace std;
const int N = 1e6 + 7;
bool cmp(int a, int b)
{
	return a > b;
}
int main()
{
	ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int a[150009] = { 0 };
		int sum[150009] = { 0 };
		int n, q;
		cin >> n >> q;
		for (int i = 1;i <= n;i++) cin >> a[i];
		sort(a + 1, a + 1 + n,cmp);
		for (int i = 1;i <= n;i++)
		{
			sum[i] = sum[i - 1] + a[i];
		}
		for (int i = 1;i <= q;i++)
		{
			int x;
			cin >> x;
			int l = 1, r = n;
			while (l < r)
			{
				int mid = (l + r) / 2;
				if (sum[mid] >= x) r = mid;
				else l = mid + 1;
			}
			if (sum[l] >= x) cout << l << endl;
			else cout << "-1" << endl;
		}
	}
	return 0;
}
