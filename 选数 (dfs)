#include<bits/stdc++.h>
#define endl '\n'
#define ll long long
#define int ll
using namespace std;
const int N=1e6+7;
const int M=2e3+7;
int n,k,sum=0,cnt=0;
int a[N],vis[N],b[N];
bool isprime(int x)
{
	if(x==1) return 0;
	if(x==2) return 1;
	for(int i=2;i*i<=x;i++)
	{
		if(x%i==0) return 0;
	}
	return 1;
}
void dfs(int x,int y)
{
	for(int i=x;i<=n;i++)
	{
		if(vis[i]==0)
		{
			vis[i]=1;
			sum+=a[i];
			if(y==k&&isprime(sum)) cnt++;
			else
			{
				dfs(i+1,y+1);
			}
			vis[i]=0;
			sum-=a[i];
		}
	}
}
signed main()
{
	ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
	cin>>n>>k;
	for(int i=1;i<=n;i++)
	{
		cin>>a[i];
	}
	dfs(1,1);
	cout<<cnt<<endl;
	return 0;
}
